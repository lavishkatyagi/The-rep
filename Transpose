for(i=0;i<2;i++)
{
  for( j=0;j<2;j++)

scanf("%d",&a[i][j]);
  }
printf("the matrix is\n");
for(i=0;i<2;i++)
{
  for( j=0;j<2;j++)
{printf("%d ",a[i][j]);
}
printf("\n");
  }


for(i=0;i<2;i++)
{
  for( j=0;j<2;j++)
     {if(i>j)
  { temp=a[j][i];
   a[j][i]=a[i][j];
   a[i][j]=temp;}
   }printf("\n");}


printf("the tranpose is\n");
for(i=0;i<2;i++)
{
  for( j=0;j<2;j++)

{printf("%d ",a[i][j]);}
